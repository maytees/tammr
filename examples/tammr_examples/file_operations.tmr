// Writing to a file
let content = "Hello, Tammr!"
writeFile("test.txt", content)
println("File written successfully")

// Reading from a file
let readContent = readFile("test.txt")
println("File content: ${readContent}")

// Appending to a file
appendFile("test.txt", "\nTammr is awesome!")
println("Content appended to file")

// Reading updated file
let updatedContent = readFile("test.txt")
println("Updated file content: ${updatedContent}")
